@model List<VideoTravel.Models.Post.Post>
@{
    ViewData["meta_title"] = "Home page";
}

<h2>Last Travel Video</h2>
<div class="d-flex flex-wrap">
    @{
        foreach (var post in Model)
        {
            <div class="p-2 col-2">
                <partial name="Post/_CardPartial" model="post"/>
            </div>
        }
    }
</div>